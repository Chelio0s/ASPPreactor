@model IEnumerable<PreactorASPCore.Models.PreactorData.InfoWH>

@{
    ViewData["Title"] = "Index";
}

<h2>Халло</h2>
<a>Дата в формате дд.мм.гггг</a>
<a>@ViewBag.date</a>
@using (Html.BeginForm())
{
    @Html.TextBox("date")
    <input type="submit" />
}

<p>
    <a> Колличестов элементов: @ViewBag.koll</a>
</p>

@if (ViewBag.koll != null)
{
<table class="table" border="1">
    <thead>
        <tr>
           <th></th>
           <th></th>
           <th colspan="2">Смена 1</th>
           <th colspan="2">Смена 2</th>
        </tr>
            <tr>
                <th></th>
                <th></th>
                    <th>С-по</th>
                    <th>Основоной перерыв</th>
                    <th>C-по</th>
                    <th>Основоной перерыв</th>
            </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Code)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Title)
            </td>
            <td>@Html.DisplayFor(modelItem => item.S1FT)</td>
            <td>@Html.DisplayFor(modelItem => item.S1break)</td>
            <td>@Html.DisplayFor(modelItem => item.S2FT)</td>
            <td>@Html.DisplayFor(modelItem => item.S2break)</td>
        </tr>
        }
    </tbody>
</table>
}